{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/",
        "dist/",
        "coverage/",
        "web"
    ],
    "watch": [
        "server/"
    ],
    "execMap": {
        "ts": "node -r ts-node/register"
    },
    "env": {
        "NODE_ENV": "development",
        "MUTE": true
    },
    "ext": "js,json,ts"
}